root = true

[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8

[*.js]
indent_style = space
indent_size = 2

[*.json]
indent_style = space
indent_size = 2

[*.html]
indent_style = space
indent_size = 2
insert_final_newline = false

[*.ttf]
insert_final_newline = false

[*.woff]
insert_final_newline = false

[*.eot]
insert_final_newline = false
