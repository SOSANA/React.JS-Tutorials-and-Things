{
  "name": "express-react-redux-webpack",
  "version": "1.0.0",
  "description": "A quick starter app for kick starting apps using Express.JS, React.JS, Redux, and Webpack",
  "main": "index.js",
  "repository": "https://github.com/SOSANA/express-react-redux-webpack",
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon index.js",
    "clean": "rimraf node_modules",
    "test": "mocha --compilers js:babel-core/register --require ./test/test.helper.js --recursive ./test",
    "test:client": "cross-env NODE_ENV=test BABEL_ENV=test npm run test",
    "test:watch": "cross-env NODE_ENV=test BABEL_ENV=test npm run test -- --watch"
  },
  "keywords": [
    "express",
    "react",
    "redux",
    "webpack"
  ],
  "author": "SOSANA",
  "license": "ISC",
  "dependencies": {
